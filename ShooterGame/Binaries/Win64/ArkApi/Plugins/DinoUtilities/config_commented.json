				"DinoFeedRangePrice": 1000,
				"DinoFeedRangeFoundations": 20,
				"DinoFeedRangeBlacklist": [
					"Blueprint'/Game/PrimalEarth/Dinos/Para/Para_Character_BP.Para_Character_BP'"
				],

