				"DinoFastpodSingleBabies": true,		
				"DinoFastpodSingleRequireCryopods": true,
				"DinoFastpodSingleBlacklist": [
					"Dodo"
				],

				"DinoFeedRangeEnabled": true,  // instantly feed all dinos withing range
